<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project ooc-frontend">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="/blue/Dev/ooc-frontend/bin/ooc.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="org.ooc.frontend.CommandLine"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="/blue/Dev/ooc-frontend/build/classes"/>
            <fileset dir="/blue/Dev/ubi/build/classes"/>
            <fileset dir="/blue/Dev/ooc/build/classes"/>
        </jar>
    </target>
</project>
